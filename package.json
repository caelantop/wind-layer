{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "tsc": "tsc",
    "prebuild": "run-p tsc lint",
    "clean": "lerna run --parallel clean",
    "lint": "tslint --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "dev": "lerna run --parallel dev",
    "dev:demo": "rollup -wm -c rollup/demo.config.js",
    "build": "lerna run --parallel build",
    "build:declarations": "lerna run --parallel tsc",
    "dev:docs": "vuepress dev docs --no-cache",
    "build:docs": "vuepress build docs --no-cache",
    "postbuild": "yarn build:declarations",
    "prerelease": "yarn build",
    "bootstrap": "lerna bootstrap",
    "test": "jest --env=node --colors --coverage test",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "precommit": "lint-staged",
    "publish-packages": "lerna publish --force-publish --ignore-changes",
    "start": "node server/bin/www",
    "prd:server": "pm2 start server/bin/www"
  },
  "lint-staged": {
    "*.{js,ts/tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "resolutions": {
    "buffer": "5.6.0",
    "@babel/compat-data": "7.13.12",
    "@babel/preset-env": "7.13.12"
  },
  "dependencies": {
    "ol": "^6.2.1",
    "@sakitam-gis/ol5": "^5.3.3"
  },
